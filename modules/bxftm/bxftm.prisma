model PrismaBxFtmWithdrawalRequest {
    id                      String              @id

    user                    String
    amount                  String
    requestTimestamp        Int
    isWithdrawn             Boolean
}

model PrismaBxFtmStakingData {
    id                      String              @id
    totalFtm                String
    totalFtmStaked          String
    totalFtmInPool          String
    numberOfVaults          Int
}